<div class="row">
  <div class="col-md-4 col-md-offset-4">
<% @locations.each do |location| %> 
<strong></strong>
	<% location.recordings.each do |recording|%> 
	<h1> Temp: <%= recording.temp%></h1>
	<h1> Status: <%= recording.status%></h1>
		<h4>Location: <%= location.name%></h4> <%= link_to "Edit", location %> <%= link_to "Delete", location, method: :delete, data: {confirm: 'Are you sure?'} %>
	<% end %>
<% end %>
</div>
</div>